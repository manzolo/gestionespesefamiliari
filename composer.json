{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": ["app/AppKernel.php"],
        "classmap": ["vendor/tecnickcom/tcpdf/tcpdf.php"]
    },
    "require": {
        "php": ">=5.4.3,<7.1",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.5",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "paragonie/random_compat": "~1.4.0",
        "fi/fifreecorebundle": "~2.0",
        "fi/pannelloamministrazionebundle": "~2.0",
        "fi/osbundle": "~2.0",
        "phpoffice/phpexcel": "~1.8",
        "phpoffice/phpword": "~0.11",
        "friendsofsymfony/user-bundle": "dev-master#47e4de9d8b6fed521729fb38107d9413863fe4ce as 2.0",
        "beberlei/DoctrineExtensions": "^1.0",
        "tufanbarisyildirim/php-apk-parser": "dev-master",
        "tecnickcom/tcpdf": "^6.2"
    },
    "require-dev": {
        "sensiolabs/security-checker": "^3.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^2.7",
        "phpmd/phpmd": "^2.4",
        "sebastian/phpcpd": "^2.0",
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7",
        "behat/behat": "3.0.*",
        "behat/mink-selenium2-driver": "1.2.*",
        "behat/symfony2-extension": "2.0.*",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "~1.1",
        "se/selenium-server-standalone": "^2.53",
        "phpunit/phpunit": "^5.5"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "keep-outdated": true
        },
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
