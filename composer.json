{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": ["app/AppKernel.php"],
        "classmap": ["vendor/tecnickcom/tcpdf/tcpdf.php"]

    },
    "require": {
        "php": ">=5.4.3,<5.7",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/inflector": "1.0.*",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "symfony/yaml": "2.8.*",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "fi/fifreecorebundle": "2.0.*",
        "fi/pannelloamministrazionebundle": "2.0.*",
        "fi/osbundle": "~2.0",
        "tecnickcom/tcpdf": "~6.0",
        "phpoffice/phpexcel": "~1.8",
        "phpoffice/phpword": "~0.11",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "phpunit/phpunit": "~4.8",
        "beberlei/DoctrineExtensions": "^1.0",
        "tufanbarisyildirim/php-apk-parser": "dev-master",
        "phpmd/phpmd": "dev-master",
        "bamboohr/phpcs": "dev-master",
        "satooshi/php-coveralls": "^1.0",
        "sebastian/phpcpd": "^2.0",
        "behat/mink": "^1.7",
        "behat/mink-selenium2-driver": "^1.3",
        "joomla-projects/selenium-server-standalone": "v2.53.1.1",
        "behat/behat": "^3.1"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "keep-outdated": true
        },
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
